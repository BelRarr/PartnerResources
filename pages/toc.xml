---
layout: null
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>

<urlset>
  {% assign tmp_docs = site.docs | sort: "url" %}{% for doc in tmp_docs %}{% if doc.nav_exclude != true %}<url>
  <id>{{ doc.id | xml_escape }}</id>
  <permalink>{{ site.url }}{{ doc.permalink | xml_escape }}</permalink>
  <loc>{{ site.url }}{{ doc.url | xml_escape }}</loc>
  <path>{{ doc.path | xml_escape }}</path>
  <title>{{ doc.title | xml_escape }}</title>
  <categories>{{ doc.categories | xml_escape }}</categories>
  <tags>{{ doc.tags | xml_escape }}</tags>
  </url>{% endif %}
  {% endfor %}
</urlset>
