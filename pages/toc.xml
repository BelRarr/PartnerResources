---
layout: null
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>

<urlset>
  {% for doc in site.docs %}{% if doc.nav_exclude != true %}{% assign tags= concat: doc.tags %}<url>
    <loc>{{ site.baseurl }}{{ doc.url | xml_escape }}</loc>
    <title>{{ doc.title | xml_escape }}</title>
    <tags>{{ tags | xml_escape }}</tags>
  </url>{% endif %}
  {% endfor %}
</urlset>
